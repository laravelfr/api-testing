{
    "name": "laravelfr/api-testing",
    "description": "A package which provide all you need for testing apis!",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Mathieu TUDISCO",
            "email": "dev@mathieutu.ovh"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelFr\\ApiTesting\\": "src/"
        }
